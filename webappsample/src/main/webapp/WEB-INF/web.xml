
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">

	<display-name>Archetype Created Web Application</display-name>
	

	<servlet>
		<servlet-name>Jstl1</servlet-name>
		<servlet-class>by.grodno.vika.webappsample.JstlServlet1</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Jstl2</servlet-name>
		<servlet-class>by.grodno.vika.webappsample.JstlServlet2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Jstl3</servlet-name>
		<servlet-class>by.grodno.vika.webappsample.JstlServlet3</servlet-class>
	</servlet>

<servlet>
		<servlet-name>Jstl4</servlet-name>
		<servlet-class>by.grodno.vika.webappsample.JstlServlet4</servlet-class>
	</servlet>



	<servlet-mapping>
		<servlet-name>Jstl4</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Jstl1</servlet-name>
		<url-pattern>/jstl1</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>Jstl2</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>Jstl3</servlet-name>
		<url-pattern>/user/delete</url-pattern>
	</servlet-mapping>
</web-app>